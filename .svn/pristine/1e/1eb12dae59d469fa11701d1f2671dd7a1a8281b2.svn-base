package models.pojo;

import java.util.Date;

import play.data.format.Formats;
import eztravel.rest.enums.member.SexType;

/**
 * 好友資料.
 * 
 * @author CJWang
 */
public class Friend {

  /** 會員編號. */
  private String custNo;

  /** 中文姓. */
  private String nameChnFirst;

  /** 中文名. */
  private String nameChnLast;

  /** 英文姓. */
  private String nameEngFirst;

  /** 英文名. */
  private String nameEngLast;

  /** 性別. */
  private SexType sex;

  /** 生日. */
  @Formats.DateTime(pattern = "yyyyMMdd")
  private Date birthday;

  /** 身分證號. */
  private String personId;

  /**
   * Gets the cust no.
   * 
   * @return 會員編號
   */
  public String getCustNo() {
    return custNo;
  }

  /**
   * Sets the cust no.
   * 
   * @param custNo 會員編號
   */
  public void setCustNo(String custNo) {
    this.custNo = custNo;
  }

  /**
   * Gets the name chn first.
   * 
   * @return 中文姓
   */
  public String getNameChnFirst() {
    return nameChnFirst;
  }

  /**
   * Sets the name chn first.
   * 
   * @param nameChnFirst 中文姓
   */
  public void setNameChnFirst(String nameChnFirst) {
    this.nameChnFirst = nameChnFirst;
  }

  /**
   * Gets the name chn last.
   * 
   * @return 中文名
   */
  public String getNameChnLast() {
    return nameChnLast;
  }

  /**
   * Sets the name chn last.
   * 
   * @param nameChnLast 中文名
   */
  public void setNameChnLast(String nameChnLast) {
    this.nameChnLast = nameChnLast;
  }

  /**
   * Gets the name eng first.
   * 
   * @return 英文姓
   */
  public String getNameEngFirst() {
    return nameEngFirst;
  }

  /**
   * Sets the name eng first.
   * 
   * @param nameEngFirst 英文姓
   */
  public void setNameEngFirst(String nameEngFirst) {
    this.nameEngFirst = nameEngFirst;
  }

  /**
   * Gets the name eng last.
   * 
   * @return 英文名
   */
  public String getNameEngLast() {
    return nameEngLast;
  }

  /**
   * Sets the name eng last.
   * 
   * @param nameEngLast 英文名
   */
  public void setNameEngLast(String nameEngLast) {
    this.nameEngLast = nameEngLast;
  }

  /**
   * Gets the sex.
   * 
   * @return 性別
   */
  public SexType getSex() {
    return sex;
  }

  /**
   * Sets the sex.
   * 
   * @param sex 性別
   */
  public void setSex(SexType sex) {
    this.sex = sex;
  }

  /**
   * Gets the birthday.
   * 
   * @return 生日
   */
  public Date getBirthday() {
    return birthday;
  }

  /**
   * Sets the birthday.
   * 
   * @param birthday 生日
   */
  public void setBirthday(Date birthday) {
    this.birthday = birthday;
  }

  /**
   * Gets the person id.
   * 
   * @return 身分證號
   */
  public String getPersonId() {
    return personId;
  }

  /**
   * Sets the person id.
   * 
   * @param personId 身分證號
   */
  public void setPersonId(String personId) {
    this.personId = personId;
  }
}
