package models.pojo;

import java.math.BigDecimal;
import java.util.LinkedList;
import java.util.List;

import eztravel.rest.pojo.htl.RoomInfo;

/**
 * 飯店資訊.
 * 
 * @author CJWang
 */
public class Hotel {

  /** 飯店編號(商品代號). */
  private String hotelCd;

  /** 飯店照片連結. */
  private String hotelPhotoLink;

  /** 飯店名稱. */
  private String hotelName;

  /** 飯店英文名稱. */
  private String englishName;

  /** 地址. */
  private String address;

  /** 評鑑分數. */
  private BigDecimal reviewScore;

  /** 房間資訊. */
  private List<Room> roomList;

  /** 剩餘單人房. */
  private LinkedList<RoomInfo> sglCandidate;

  /** 剩餘雙人房. */
  private LinkedList<RoomInfo> twnCandidate;

  /** 剩餘三人房. */
  private LinkedList<RoomInfo> trpCandidate;

  /** 剩餘四人房. */
  private LinkedList<RoomInfo> qudCandidate;

  /**
   * Gets the room list.
   * 
   * @return 房間資訊
   */
  public List<Room> getRoomList() {
    return roomList;
  }

  /**
   * Sets the room list.
   * 
   * @param roomList 房間資訊
   */
  public void setRoomList(List<Room> roomList) {
    this.roomList = roomList;
  }

  /**
   * Gets the hotel photo link.
   * 
   * @return 飯店照片連結
   */
  public String getHotelPhotoLink() {
    return hotelPhotoLink;
  }

  /**
   * Sets the hotel photo link.
   * 
   * @param hotelPhotoLink 飯店照片連結
   */
  public void setHotelPhotoLink(String hotelPhotoLink) {
    this.hotelPhotoLink = hotelPhotoLink;
  }

  /**
   * Gets the hotel name.
   * 
   * @return 飯店名稱
   */
  public String getHotelName() {
    return hotelName;
  }

  /**
   * Sets the hotel name.
   * 
   * @param hotelName 飯店名稱
   */
  public void setHotelName(String hotelName) {
    this.hotelName = hotelName;
  }

  /**
   * Gets the address.
   * 
   * @return 地址
   */
  public String getAddress() {
    return address;
  }

  /**
   * Sets the address.
   * 
   * @param address 地址
   */
  public void setAddress(String address) {
    this.address = address;
  }

  /**
   * Gets the hotel cd.
   * 
   * @return 飯店編號(商品代號)
   */
  public String getHotelCd() {
    return hotelCd;
  }

  /**
   * Sets the hotel cd.
   * 
   * @param hotelCd 飯店編號(商品代號)
   */
  public void setHotelCd(String hotelCd) {
    this.hotelCd = hotelCd;
  }

  /**
   * Gets the review score.
   * 
   * @return 評鑑分數
   */
  public BigDecimal getReviewScore() {
    return reviewScore;
  }

  /**
   * Sets the review score.
   * 
   * @param reviewScore 評鑑分數
   */
  public void setReviewScore(BigDecimal reviewScore) {
    this.reviewScore = reviewScore;
  }

  /**
   * Gets the sgl candidate.
   * 
   * @return 剩餘單人房
   */
  public LinkedList<RoomInfo> getSglCandidate() {
    return sglCandidate;
  }

  /**
   * Sets the sgl candidate.
   * 
   * @param sglCandidate 剩餘單人房
   */
  public void setSglCandidate(LinkedList<RoomInfo> sglCandidate) {
    this.sglCandidate = sglCandidate;
  }

  /**
   * Gets the twn candidate.
   * 
   * @return 剩餘雙人房
   */
  public LinkedList<RoomInfo> getTwnCandidate() {
    return twnCandidate;
  }

  /**
   * Sets the twn candidate.
   * 
   * @param twnCandidate 剩餘雙人房
   */
  public void setTwnCandidate(LinkedList<RoomInfo> twnCandidate) {
    this.twnCandidate = twnCandidate;
  }

  /**
   * Gets the trp candidate.
   * 
   * @return 剩餘三人房
   */
  public LinkedList<RoomInfo> getTrpCandidate() {
    return trpCandidate;
  }

  /**
   * Sets the trp candidate.
   * 
   * @param trpCandidate 剩餘三人房
   */
  public void setTrpCandidate(LinkedList<RoomInfo> trpCandidate) {
    this.trpCandidate = trpCandidate;
  }

  /**
   * Gets the qud candidate.
   * 
   * @return 剩餘四人房
   */
  public LinkedList<RoomInfo> getQudCandidate() {
    return qudCandidate;
  }

  /**
   * Sets the qud candidate.
   * 
   * @param qudCandidate 剩餘四人房
   */
  public void setQudCandidate(LinkedList<RoomInfo> qudCandidate) {
    this.qudCandidate = qudCandidate;
  }

  /**
   * Gets the english name.
   * 
   * @return 飯店英文名稱
   */
  public String getEnglishName() {
    return englishName;
  }

  /**
   * Sets the english name.
   * 
   * @param englishName 飯店英文名稱
   */
  public void setEnglishName(String englishName) {
    this.englishName = englishName;
  }
}
