color: #202020;
}

.search-result ul {
list-style: none;
margin: 0;
padding: 0px;	
overflow: hidden; /* 超過範圍隱藏 */
white-space: nowrap; /* 不斷行 */
}

.search-result ul li {
padding: 0px 8px 0px 8px;
display:inline;
vertical-align: middle;
}


/* HSR 專用(Orange) */
.search-result .hsr .title
{
/*
background:url(../../images/icon/hotel/searchResultHsrLine.png) no-repeat 10px 20px;
*/
height: 30px;
margin: 0px 0px 2px 15px;
padding: 0px;
background:url(../../images/icon/hotel/searchResultHsrLine.jpg) repeat-x 12px 23px;
width: 90%;
}

.search-result .hsr span
{
background-color: #FF9046;
}

.search-result .hsr ul li {
background:url(../../images/icon/hotel/searchResultHsrDot.png) no-repeat 0px 4px;
}

/* HTL 專用(Green) */
.search-result .htl .title
{
/*
background:url(../../images/icon/hotel/searchResultHtlLine.png) no-repeat 10px 20px;
*/
height: 30px;
margin: 0px 0px 2px 15px;
padding: 0px;
background:url(../../images/icon/hotel/searchResultHtlLine.jpg) repeat-x 12px 23px;
width: 90%;
}

.search-result .htl span
{
background-color: #5CCA04;
}

.search-result .htl ul li {
background:url(../../images/icon/hotel/searchResultHtlDot.png) no-repeat 0px 4px;
}

@media (max-width: 768px) {
  .search-result .hsr, .search-result .htl {
    padding: 10px 0px 10px 30px;
  }
}



