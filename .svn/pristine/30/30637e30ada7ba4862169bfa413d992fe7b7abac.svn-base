/* GLOBAL STYLES
-------------------------------------------------- */

body {
/* 覆寫 - 搜尋頁面灰背景 */
background: #f2f2f2;
}

/*
button,
 */
input,
select,
textarea {
font-family: Arial, Verdana, sans-serif, '新細明體';
font-size: 13px;
height: 30px;
line-height: 30px;
color: #202020;
border: 1px solid #cccccc;
background-color: #ffffff;
background-image: none;
vertical-align: middle;
padding: 4px 8px;
/*border-radius: 3px;*/
display: block;
width: 100%;
-webkit-box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075);
        box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075);
-webkit-transition: border-color ease-in-out 0.15s, box-shadow ease-in-out 0.15s;
        transition: border-color ease-in-out 0.15s, box-shadow ease-in-out 0.15s;
}

input:focus,
select:focus,
textarea:focus {
border-color: #66afe9;
outline: 0;
-webkit-box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075), 0 0 8px rgba(102, 175, 233, 0.6);
        box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075), 0 0 8px rgba(102, 175, 233, 0.6);
}

textarea {
height: auto;
}

input[type="checkbox"],
input[type="radio"] {
display: inline;
display: inline-block;
height: auto;
line-height: auto;
padding: 0;
box-sizing: border-box;
border: 0px transparent;
background: transparent;
-webkit-box-shadow: 0px 0px 0px rgba(0,0,0,1);
        box-shadow: 0px 0px 0px rgba(0,0,0,1);
}

input[type="radio"],
input[type="checkbox"] {
margin: 4px 0 0;
margin-top: 1px \9;
/* IE8-9 */
line-height: normal;
}

input[type="file"]:focus,
input[type="radio"]:focus,
input[type="checkbox"]:focus {
outline: thin dotted #333;
outline: 5px auto -webkit-focus-ring-color;
outline-offset: -2px;
}








.search-form {
min-height: 20px;
padding: 19px;
margin-bottom: 20px;

background-color: #ffffff;
border: 2px solid #bfeb77;
border-radius: 8px;
background:rgba(255,255,255,0.85);
background: transparent\9;
zoom:1\8; /* required for the filters */
-ms-filter:"progid:DXImageTransform.Microsoft.gradient(startColorstr=#ffffff, endColorstr=#ffffff)"; /* IE8 */
filter: progid:DXImageTransform.Microsoft.gradient(startColorstr=#ffffff, endColorstr=#ffffff); /* IE5.5-7 */
-webkit-box-shadow: inset 0 1px 3px rgba(0, 0, 0, 0.05), 0 1px 0 rgba(255, 255, 255, 0.1);
        box-shadow: inset 0 1px 3px rgba(0, 0, 0, 0.05), 0 1px 0 rgba(255, 255, 255, 0.1);
}

.search-form label {
background:url(../../images/icon/hotel/searchFormDot.png) no-repeat 0px 3px;
height: 25px;

margin: 5px 0px 5px 0px;
padding: 0px 0px 0px 30px;
line-height: 25px;
vertical-align: middle;
font-weight: bold;
color: #202020;
}

.search-form-line {
background:url(../../images/icon/hotel/searchFormTitleLine.png) no-repeat 152px 13px;
}

.search-form label.none{
background-image: none;
background-color:transparent;
}

.search-form input.none{
border: 0px transparent;
background: transparent;
-webkit-box-shadow: 0px 0px 0px rgba(0,0,0,1);
        box-shadow: 0px 0px 0px rgba(0,0,0,1);
}

.search-form input:focus.none {
border: 0px transparent;
background: transparent;
-webkit-box-shadow: 0px 0px 0px rgba(0,0,0,1);
        box-shadow: 0px 0px 0px rgba(0,0,0,1);
}

.search-form-removeRoom {
background:url(../../images/icon/hotel/searchFormRemove.png) no-repeat 0px 0px;
border: 0px transparent;
height: 24px;  
width: 24px;
}

.search-form-submit {
background:url(../../images/icon/hotel/searchFormSearch.png) no-repeat 0px 0px;
border: 0px transparent;
height: 43px;  
width: 128px;
}

