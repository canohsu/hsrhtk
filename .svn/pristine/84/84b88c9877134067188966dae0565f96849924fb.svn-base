.search-area {
    max-height: 640px;
    background-image: url(../../images/common/sample01.jpg);
    padding-top: 20px;
    font-size: 1em;
}

.control-label {
    font-weight: normal;
}

/* Del By Kiki
.search-form {
    min-height: 20px;
    padding: 19px;
    margin-bottom: 20px;
    background: rgba(245, 245, 245, 0.75);
}
*/

.crumb {
    /* Del By Kiki
    padding-top: 15px;
    padding-left: 15px;
    padding-bottom: 10px;
    */
    padding: 6px 0px 6px 0px;
}

.search-result-block {
    margin-left: 12px;
    margin-right: 12px;
    padding: 19px 3px 19px 3px;
}

/* event-area */
.event-area {
    padding-bottom: 25px;
    border-bottom: 1px solid #EAEAEA; 
}

.event-row {
    padding-top: 25px;
}

.event-row > div > img {
    max-height:100%;
    max-width:100%;
}

@media (min-width: 768px) {
    .content {
        /* Del By Kiki
        font-size: 1em;
        */
    }
    .search-result-block {
        margin-right: auto;
        margin-left: auto;
        padding: 19px 3px 19px 3px;
    }
    .crumb {
        padding-left: 0px;
    }
    h1,
    .h1 {
      font-size: 36px;
    }

    h2,
    .h2 {
      font-size: 30px;
    }

    h3,
    .h3 {
      font-size: 24px;
    }

    h4,
    .h4 {
      font-size: 18px;
    }

    h5,
    .h5 {
      font-size: 14px;
    }

    h6,
    .h6 {
      font-size: 12px;
    }
}

