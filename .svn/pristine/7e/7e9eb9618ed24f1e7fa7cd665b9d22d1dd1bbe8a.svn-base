  /**
   * 取得總票數
   * 
   * @return
   */
  public int getTotalTicketQty() {
    int qty = 0;
    if (null != this.sglRooms) {
      for (Room room : this.sglRooms) {
        qty += room.getFull();
        qty += room.getChildren();
        qty += room.getSenior();
        qty += room.getHalf();
      }
    }
    if (null != this.twnRooms) {
      for (Room room : this.twnRooms) {
        qty += room.getFull();
        qty += room.getChildren();
        qty += room.getSenior();
        qty += room.getHalf();
      }
    }
    if (null != this.trpRooms) {
      for (Room room : this.trpRooms) {
        qty += room.getFull();
        qty += room.getChildren();
        qty += room.getSenior();
        qty += room.getHalf();
      }
    }
    if (null != this.qudRooms) {
      for (Room room : this.qudRooms) {
        qty += room.getFull();
        qty += room.getChildren();
        qty += room.getSenior();
        qty += room.getHalf();
      }
    }
    return qty;
  }

  /**
