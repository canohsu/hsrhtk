package models.pojo;

import java.util.List;
import java.util.Map;

import eztravel.rest.pojo.hsr.HsrProductInfo;
import eztravel.rest.pojo.hsr.HsrScheduleInfo;

/**
 * 前端查詢條件.
 * 
 * @author CJWang
 */
public class HsrHtl extends Order {

  /** 目的城市名稱. */
  private String arrCityName;

  /** 去程高鐵資訊. */
  private HsrProductInfo depHsrInfo;

  /** 去程高鐵時段. */
  private String depPeriod;

  /** 去程高鐵班次表. */
  private Map<String, List<HsrScheduleInfo>> depScheduleMap;

  /** 回程高鐵資訊. */
  private HsrProductInfo returnHsrInfo;

  /** 回程高鐵時段. */
  private String returnPeriod;

  /** 回程高鐵班次表. */
  private Map<String, List<HsrScheduleInfo>> returnScheduleMap;

  /** 飯店資訊. */
  private List<Hotel> hotelList;

  /**
   * Gets the arr city name.
   * 
   * @return 目的城市名稱
   */
  public String getArrCityName() {
    return arrCityName;
  }

  /**
   * Sets the arr city name.
   * 
   * @param arrCityName 目的城市名稱
   */
  public void setArrCityName(String arrCityName) {
    this.arrCityName = arrCityName;
  }

  /**
   * Gets the dep hsr info.
   * 
   * @return 去程高鐵資訊
   */
  public HsrProductInfo getDepHsrInfo() {
    return depHsrInfo;
  }

  /**
   * Sets the dep hsr info.
   * 
   * @param depHsrInfo 去程高鐵資訊
   */
  public void setDepHsrInfo(HsrProductInfo depHsrInfo) {
    this.depHsrInfo = depHsrInfo;
  }

  /**
   * Gets the return hsr info.
   * 
   * @return 回程高鐵資訊
   */
  public HsrProductInfo getReturnHsrInfo() {
    return returnHsrInfo;
  }

  /**
   * Sets the return hsr info.
   * 
   * @param returnHsrInfo 回程高鐵資訊
   */
  public void setReturnHsrInfo(HsrProductInfo returnHsrInfo) {
    this.returnHsrInfo = returnHsrInfo;
  }

  /**
   * Gets the dep schedule map.
   * 
   * @return 去程高鐵班次表
   */
  public Map<String, List<HsrScheduleInfo>> getDepScheduleMap() {
    return depScheduleMap;
  }

  /**
   * Sets the dep schedule map.
   * 
   * @param depScheduleMap 去程高鐵班次表
   */
  public void setDepScheduleMap(Map<String, List<HsrScheduleInfo>> depScheduleMap) {
    this.depScheduleMap = depScheduleMap;
  }

  /**
   * Gets the return schedule map.
   * 
   * @return 回程高鐵班次表
   */
  public Map<String, List<HsrScheduleInfo>> getReturnScheduleMap() {
    return returnScheduleMap;
  }

  /**
   * Sets the return schedule map.
   * 
   * @param returnScheduleMap 回程高鐵班次表
   */
  public void setReturnScheduleMap(Map<String, List<HsrScheduleInfo>> returnScheduleMap) {
    this.returnScheduleMap = returnScheduleMap;
  }

  /**
   * Gets the dep period.
   * 
   * @return 去程高鐵時段
   */
  public String getDepPeriod() {
    return depPeriod;
  }

  /**
   * Sets the dep period.
   * 
   * @param depPeriod 去程高鐵時段
   */
  public void setDepPeriod(String depPeriod) {
    this.depPeriod = depPeriod;
  }

  /**
   * Gets the return period.
   * 
   * @return 回程高鐵時段
   */
  public String getReturnPeriod() {
    return returnPeriod;
  }

  /**
   * Sets the return period.
   * 
   * @param returnPeriod 回程高鐵時段
   */
  public void setReturnPeriod(String returnPeriod) {
    this.returnPeriod = returnPeriod;
  }

  /**
   * Gets the hotel list.
   * 
   * @return 飯店資訊
   */
  public List<Hotel> getHotelList() {
    return hotelList;
  }

  /**
   * Sets the hotel list.
   * 
   * @param hotelList 飯店資訊
   */
  public void setHotelList(List<Hotel> hotelList) {
    this.hotelList = hotelList;
  }
}
